//汉诺塔问题
#include<bits/stdc++.h>
using namespace std;
int count=0;
int m;
//s1 起始位 s2 过渡位 s3 目的位 
void Hanio(char s1,char s2,char s3,int n)
{
	if(n==1)
	{
		count++;
        if(count==m)
    cout<<'#'<<m<<<<':'<<"s1"<<"->">>"s3";
	}
	else
	{
		Hanio(s1,s3,s2,n-1);
    count++;
    if(cnt == m)cout << "#" << m << ": " << A << "->" << C << '\n';
		Hanio(s2,s1,s3,n-1);
	}
}
int main()
{
  int n;
  cin>>n>>m;
	Hanio('A','B','C',n);
  cout<<count;
	
} 
