冒泡排序：
#include<stdio.h>
void maopao(int* arr, int sz)
{
	int i, j;
	for (int i = 0;i < sz - 1;i++)
	{
		for (int j = 0;j < sz - i - 1;j++)
		{
			if (arr[j] > arr[j + 1])
			{
				int temp = arr[j];
				arr[j] = arr[j + 1];
				arr[j + 1] = temp;
			}
		}
	}
}
int main()
{
	int arr[] = { 2,6,4,6,98,5,0,58,5, };
	int sz = sizeof(arr) / sizeof(arr[0]);
	maopao(arr, sz);
	for (int i = 0;i < sz;i++)
	{
		printf("%d ", arr[i]);
	}
}
